      },
      "source": [
        "def split_to_batches(data):\n",
        "  number_of_batches = int(np.ceil(len(data) / BATCH_SIZE))\n",
        "  batches = []\n",
        "  for i in range(0, number_of_batches):\n",
        "    batch = data[i * BATCH_SIZE : (i + 1) * BATCH_SIZE]\n",
        "    batches.append(batch)\n",
        "  return batches"
      ],
      "execution_count": 41,
      "outputs": []
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "Nq0E2f2u3dQb",
        "colab_type": "code",
